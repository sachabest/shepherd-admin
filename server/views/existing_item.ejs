<div class='row'>
	<div class='twelve columns'>
		<table class='u-full-width'>
			<thead>
				<tr>
					<th>Complaint</th>
					<th>Diagnosis</th>
					<th>Prescription</th>
					<th>Treatment</th>
					<th>Test</th>
				</tr>
			</thead>
			<tbody>
				<% for (var index = 0; index < items.length; index++) { %>
					<tr>
					<% if (items[index].complaint) { %>
						<td><div><%=items[index].name %></div>
						<div>items[index].category%></div></td>
					<% } if (items[index].diagnosis) { %>
						<td><div><%=items[index].name %></div></td>
					<% } if (items[index].test) { %>
						<td><div><%=items[index].name %></div>
						<div><%=items[index].price %></div></td>
					<% } if (items[index].prescription) { %>
						<td><div><%=items[index].name %></div>
						<div><%=items[index].price %></div></td>
					<% } if (items[index].treatment) { %>
						<td><div><%=items[index].name %></div>
						<div><%=items[index].category %></div>
						<div><%=items[index].price %></div></td>
					<% } %>
					</tr>
  				<% }; %>
			</tbody>
		</table>
	</div>
</div>